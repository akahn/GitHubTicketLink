{
  "name": "GitHub Ticket Link",
  "version": "0.1.0",
  "description": "Link from GitHub commits to third-party bug trackers",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/commit/*"],
      "js": ["jquery.js", "script.js"]
    }
  ],
  "background_page": "options.html",
  "options_page": "options.html"
}

